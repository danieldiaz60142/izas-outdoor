    let gmarkers = [], cityCircle = null, listDataSearch = [], infowindow, midpointCenter, initializedMap, LatLngbounds;

    function initMap () {
        let listStore = JSON.parse(document.querySelector(('[data-store-json]')).innerHTML);
        let lat_center = {{ section.settings.lat_center }};
        let lng_center = {{ section.settings.lng_center }};

        let center = new google.maps.LatLng(lat_center, lng_center)
        let mapOptions = {
            zoom: {{ section.settings.default_zoom_map }},
            center: center,
            mapTypeId: "roadmap"
        }

        infowindow = new google.maps.InfoWindow();
        LatLngbounds = new google.maps.LatLngBounds();

        map = new google.maps.Map(document.getElementById('map_google'), mapOptions)
        for (let key in listStore) {
            let marker = listStore[key]
            if (marker.lat && marker.lng && marker.name && marker.address) {
                addMarker(marker)
                if (marker.name) {
                    listDataSearch.push(marker.name)
                }
                if (marker.address) {
                    listDataSearch.push(marker.address)
                }
            }
        }

        map.setOptions({ styles: mapStyles['{{section.settings.map_style}}'] });
        initializedMap = map;

        initSearchBar();
    }

    function infoCallback (content, marker) {
        let storeLive = document.getElementById(marker.name.replaceAll(' ', '')),
            span = document.createElement('span'),
            stringDistance = ''
        if (storeLive) {
            stringDistance = storeLive.querySelector('.search_distance_information').innerHTML
            span.innerHTML = content
            span.querySelector('.search_distance_information').innerHTML = stringDistance
            content = span.innerHTML
        }
        infowindow.setContent(content)
        infowindow.open(map, marker)
    }

    function addMarker (marker) {
        let contentInfoStore = '<div class="info-map-store" id="info-map-store-' + marker.name + '">'
        if (marker.name) {
            contentInfoStore += '<div class="infor-window-map name-store"><span>' + marker.name + '</span><span class="search_distance_information"></span></div>'
        }
        if (marker.address) {
            contentInfoStore += '<div class="infor-window-map">' + marker.address + ' </div>'
        }
        if (marker.phone) {
            contentInfoStore += '<div class="infor-window-map"><a class="decoration-none" href="tel:' + marker.phone + '">' + marker.phone + '</a></div>'
        }
        if (marker.closing_time) {
            contentInfoStore += '<div class="infor-window-map">{{ 'general.map.closing_time' | t }}' + ' ' + marker.closing_time + ' </div>'
        }

        contentInfoStore += '<div class="directions"><a href="https://maps.google.com/maps?saddr=&daddr=' + marker.address + '" target="_blank">{{ 'general.map.directions' | t }}</a></div>'
        contentInfoStore += '</div>'
        let icon = marker.icon,
            iconMapDefault = {{ section.settings.image_icon_map_default | img_url: '50x50' | json }};

        if (icon == null && iconMapDefault) {
            icon = iconMapDefault
        }

        let pos = new google.maps.LatLng(marker.lat, marker.lng)
        marker = new google.maps.Marker({
            position: pos,
            name: marker.name,
            address: marker.address,
            lat: marker.lat,
            lng: marker.lng,
            zipcode: marker.zipcode,
            icon: icon,
            contentInfoStore: contentInfoStore,
            map: map
        })

        gmarkers.push(marker)
        marker.addListener('click', () => {
            infoCallback(contentInfoStore, marker);
            let markerId = marker.name.replace(/\s/g, "");
            let allStores = document.querySelectorAll('.store-info');
            allStores.forEach(function (element) {
                element.classList.remove('lsl-highlight');
            });
            document.getElementById(markerId).classList.add('lsl-highlight');
            var target = document.getElementById(markerId);
            window.target = document.getElementById(markerId);

            let listInfoStore = document.getElementById('list-info-store');

            listInfoStore.scrollTo({
                top: target.offsetTop - listInfoStore.offsetTop,
                behavior: 'smooth'
            });

            map.setZoom({{ section.settings.default_zoom_map | plus: 3 }})
            map.setCenter(marker.getPosition())
        })
    }

    function initSearchBar() {
        const searchInput = document.getElementById("input-search-map");
        const searchBox = new google.maps.places.SearchBox(searchInput);
        let radius, searchQuery;

        searchBox.addListener("places_changed", () => {
            const places = searchBox.getPlaces();

            if (places.length === 0) return;

            const place = places[0];
            document.getElementById('input-search-map').addEventListener('keypress', function(event) {
                // Check if the pressed key is Enter (key code 13)
                if (event.key === 'Enter') {
                    document.getElementById('btn-search').click();
                }
            });


            document.getElementById('btn-search').addEventListener('click', function (e) {
                e.preventDefault();

                radius = parseFloat(document.getElementById('google-map-radius').value);
                searchQuery = searchInput.value;

                if (searchQuery == '') {
                    initMap();
                    document.getElementById('notStoreSearch').classList.add('tw-hidden');
                    document.querySelectorAll('.search_distance_information').forEach(item => {
                        item.innerHTML = '';
                    })
                    let allStores = document.querySelectorAll('.store-info');
                    allStores.forEach(function (element) {
                        element.classList.remove('hidden')
                    });
                } else {
                    let showResultText = document.querySelector('.header-query-search');
                    let showKeySearch = showResultText.querySelector('#key_query_search');
                    showKeySearch.innerHTML = '"' + searchQuery + '"';
                    showResultText.classList.remove('hidden');

                    handlePlacesChanged(places, radius);
                    searchStoresByDistance(searchQuery, radius);
                }
            })
        });
    }

    function searchStoresByDistance(searchQuery, radius) {

        const geocoder = new google.maps.Geocoder();

        // Use the Google Maps Geocoder to geocode the searched location
        geocoder.geocode({ address: searchQuery, bounds: this.bounds }, (results, status) => {
            if (status === 'OK') {
                const searchLocation = results[0].geometry.location;
                const searchedPos = createLngLatObject(searchLocation);

                // Add a marker at the searched location
                let iconMap = {{ section.settings.image_icon_map_default | img_url: '50x50' | json }};
                const markerForSearchInput = new google.maps.Marker({
                    map: map,
                    opacity: 0.2,
                    position: searchLocation,
                    icon: {
                        url: iconMap,
                    },
                });

                showStoresNearby(searchedPos, radius);
            } else if (status === 'ZERO_RESULTS') {
                console.log('zero results, searching storeArray');
                LatLngbounds = new google.maps.LatLngBounds();

                const hasFoundStore = this.storeArray.some((store) => {
                    //returns store if ANY value of store is equal to what we're searching for.
                    const matchesSearch = Object.values(store).some((value) => {
                        try {
                            return value.toString().includes(searchQuery);
                        } catch {
                            return false;
                        }
                    });

                    toggleStoreView(store, matchesSearch);

                    return matchesSearch;
                });
                if (!hasFoundStore) return console.log('cant find search term');
                showOnlyStoresInBound(radius);
            } else {
                console.error('Geocode was not successful for the following reason:', status);
            }
        });
    }

    function createLngLatObject(obj) {
        return { lat: obj.lat(), lng: obj.lng() };
    }

    function showStoresNearby(location, radius) {
        LatLngbounds = new google.maps.LatLngBounds();
        let noResult = true;
        let distance, distanceMessage, showDistance, storeId, storePanel;

        gmarkers.forEach((store) => {
            const distanceFromSearch = calculateDistance(location, store);
            const isWithingRange = distanceFromSearch <= radius;

            if(isWithingRange) {
                noResult = false;

                distance = (parseFloat(distanceFromSearch) / 1000 ).toFixed(2);
                distanceMessage = 'Est a ' + distance + 'Km' + ' de tu bsqueda.';

                storeId = store.name.replace(/\s+/g, '');
                storePanel = document.getElementById(storeId);
                storePanel.querySelector('.search_distance_information').innerHTML = distanceMessage;
            }

            toggleStoreView(store, isWithingRange);
        });

        if(noResult) {
            document.getElementById('notStoreSearch').classList.remove('tw-hidden');
        } else {
            document.getElementById('notStoreSearch').classList.add('tw-hidden');
        }

        showOnlyStoresInBound(radius);
    }

