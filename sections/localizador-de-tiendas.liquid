        const deltaLng = toRadians(point2.lng - point1.lng);

        const a =
            Math.sin(deltaLat / 2) * Math.sin(deltaLat / 2) +
            Math.cos(lat1) * Math.cos(lat2) * Math.sin(deltaLng / 2) * Math.sin(deltaLng / 2);

        const c = 2 * Math.atan2(Math.sqrt(a), Math.sqrt(1 - a));

        return earthRadiusMeters * c;
    }

    function toRadians(degrees) {
        return degrees * (Math.PI / 180);
    }

    function toggleStoreView(store, bool) {
        let storeName, storeId;
        storeName = store.name;
        storeId = storeName.replace(/\s+/g, '');

        if (bool) {
            document.getElementById(storeId).classList.remove('hidden');
        } else {
            document.getElementById(storeId).classList.add('hidden');
        }
    }

    function showOnlyStoresInBound(radius) {
        const currentZoom = initializedMap.getZoom();
        const settingsZoom = getZoomLevel(radius);

        if (currentZoom > settingsZoom) {
            initializedMap.setZoom(settingsZoom);
        }
    }

    function closeAllInfoWindows() {
        gmarkers.forEach(function (store) {
            if (store.infoWindow) {
                store.infoWindow.close();
            }
        });
    }

    function handlePlacesChanged(places, radius) {
        if (places?.length == (0 || null || undefined)) {
            return console.log('No places');
        }

        const zoomLevel = getZoomLevel(radius);
        const place = places[0];

        initializedMap.setCenter(place.geometry.location);
        initializedMap.setZoom(zoomLevel);
    }

    function getZoomLevel(viewDistance) {
        const radius = parseFloat(viewDistance) * 40;
        const earthCircumference = 40075017; // Earth's circumference in meters
        const zoomLevel = Math.round(Math.log2(earthCircumference / (radius * 2 * Math.PI / 360)));

        return zoomLevel;
    }

    function getContactStore (titleStore) {
        searchStore(titleStore, 'zoom')
    }

    async function searchStore (value, event) {
        for (let i = 0; i < gmarkers.length; i++) {
            var marker = gmarkers[i]
            if (marker.name.toLowerCase().replaceAll(' ', '').replaceAll('\'', '').normalize('NFD').replace(/[\u0300-\u036f]/g, '') == value.toLowerCase().replaceAll(' ', '').replaceAll('\'', '').normalize('NFD').replace(/[\u0300-\u036f]/g, '')) {
                map.setZoom({{ section.settings.default_zoom_map | plus: 3 }})
                map.setCenter(marker.getPosition())
                infoCallback(marker.contentInfoStore, marker);
                let markerId = marker.name.replace(/\s/g, "");
                let allStores = document.querySelectorAll('.store-info');
                allStores.forEach(function (element) {
                    element.classList.remove('lsl-highlight');
                });
                document.getElementById(markerId).classList.add('lsl-highlight');
                showOrHiddenListStore(document.getElementById('btn-hidden-list'),'none');
            }
        }
    }

    function showOrHiddenListStore (el, type) {
        let listStore = document.getElementById('list-info-store'),
            section_map = document.getElementById('section-map');
        if (listStore) {
            if (type == 'block') {
                listStore.classList.remove('disabled');
                listStore.classList.add('show');
                section_map.classList.add('disabled');

                let listInfoStore = document.getElementById('list-info-store');

                if (typeof target !== 'undefined' && target !== null) {
                    listInfoStore.scrollTo({
                        top: target.offsetTop - listInfoStore.offsetTop,
                        behavior: 'smooth'
                    });
                }
            } else {
                listStore.classList.add('disabled');
                listStore.classList.remove('show');
                section_map.classList.remove('disabled');
            }
        }
        el.closest('.action-list-store').querySelector('.active').classList.remove('active')
        el.classList.add('active')
    }
    function bounce(id) {
        for (let i = 0; i < gmarkers.length; i++) {
            if (gmarkers[i].name.replace(/\s/g, "") == id) {
                gmarkers[i].setAnimation(google.maps.Animation.BOUNCE);
                setTimeout(function() {
                    gmarkers[i].setAnimation(null);
                }, 500);
            }
        }
    }


