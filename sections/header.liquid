{% # theme-check-disable LiquidTag, TemplateLength %}


{%- liquid
  assign has_mega_nav_pills = false
  assign has_mega_nav_columns = false
  assign has_mega_nav_sidebar = false

  if section.blocks.size > 0
    for block in section.blocks
      if block.type == "pills"
        assign has_mega_nav_pills = true
      elsif block.type == "columns"
        assign has_mega_nav_columns = true
      elsif block.type == "sidebar"
        assign has_mega_nav_sidebar = true
      endif
    endfor
  endif

  assign search_is_minimised = false
  if section.settings.minimise_search_mobile or section.settings.minimise_search_desktop
    assign search_is_minimised = true
  endif

  assign show_search_types = false
  if settings.show_search_types and shop.types
    assign show_search_types = true
  endif
-%}

{%- style -%}
  .header {
  --bg-color: {{ section.settings.bg_color.rgba }};
  --text-color: {{ section.settings.text_color.rgb }};
  --nav-bg-color: {{ section.settings.bg_color.rgb }};
  --nav-text-color: {{ section.settings.text_color.rgb }};
  --nav-child-bg-color:  {{ section.settings.bg_color.rgb }};
  --nav-child-text-color: {{ section.settings.text_color.rgb }};
  --header-accent-color: {{ section.settings.accent_color.rgb }};
  --search-bg-color: {{ section.settings.bg_color | color_mix: section.settings.text_color, 90 }};
  {% unless section.settings.logo %}
    --logo_type_scale: var(--body-font-size) * {{ section.settings.logo_type_scale }};
    --logo_font-size: calc((var(--logo_type_scale) / 1.5) * var(--fluid-1-5));
  {% endunless %}
  {% if settings.cart_shaking and cart != empty %}
    --shake-delay: 4s;
  {% endif %}
  }
  {% if template.name == "index" %}
    {% if section.settings.enable_tranparent_header != true %}
      #main-content .shopify-section:first-child{
        margin-top: var(--header-height);
      } 
    {%- endif -%}
  {%- endif -%}

{%- endstyle -%}


<store-header id="storeHeader" class="header bg-theme-bg text-theme-text {% if section.settings.main_menu_alignment == 'left-center' %} {% endif %} {% if section.settings.enable_sticky or search_is_minimised %} has-motion{% endif %}{% if search_is_minimised %} search-is-collapsed{% endif %}"
        {%- if section.settings.enable_sticky -%} data-is-sticky="true"{%- endif -%}
        {%- if search_is_minimised -%} data-is-search-minimised="true"{%- endif -%}

              style="--header-transition-speed: 300ms">
  <header class="header__grid{% if section.settings.logo_center %} header__grid--centered-logo{% else %} header__grid--left-logo{% endif %} container flex {% if section.settings.main_menu_alignment != 'left-center' %}  flex-wrap {% endif %} items-center {% if section.settings.main_menu_alignment == 'left-center' %} ls:!tw-px-[38px] !tw-px-[20px] !tw-flex !tw-justify-between {% endif %}"
          {% if section.settings.main_menu_alignment == 'left-center' %} data-header-align {% if section.settings.mobile_menu_position == 'burger-left' %} logo-align-left {% endif %}{% endif %} >
    <div class="header__logo logo
    {% if section.settings.logo_center %} text-center{% endif %} flex js-closes-menu
    {% if section.settings.main_menu_alignment == 'left-center' %} !tw-w-fit !tw-max-w-fit !tw-m-0{% endif %}
    ">
    {%- if request.page_type == 'index' and section.settings.logo_h1 -%}
      <h1 class="logo__h1 m-0">
        {%- endif -%}
        <a class="logo__link inline-block" href="{{ routes.root_url }}">
          {%- if section.settings.logo != blank -%}
            {%- liquid
              assign logo_width = section.settings.logo_width
              if section.settings.logo.width < logo_width
                assign logo_width = section.settings.logo.width
              endif
              assign alt_text = section.settings.logo.alt | default: shop.name
            -%}
            <span {% if template.name == "index" %}id="storeHeaderLogo"{% endif %} class="flex" style="max-width: {{ section.settings.logo_width | plus: 0 }}px;">
              {% render 'image',
                      image: section.settings.logo,
                      src_width: logo_width,
                      srcset_2x: true,
                      lazy_load: false,
                      alt_text: alt_text
              %}
            </span>
          {%- else -%}
            <span class="logo__text h2 text-theme-text m-0">{%- if section.settings.logo_text != blank -%}{{- section.settings.logo_text -}}{%- else -%}{{ shop.name }}{%- endif -%}</span>
          {%- endif -%}
        </a>
        {%- if request.page_type == 'index' and section.settings.logo_h1 -%}
      </h1>
      {%- endif -%}
    </div>

    {%- if section.settings.main_menu_alignment == 'left-center' -%}
      {%- if section.settings.mobile_menu_position == 'burger-right' -%}
        {%- if section.settings.menu != blank -%}
          {%- assign total_promo_count = 0 -%}
          {%- assign featured_links = section.settings.menu_featured_link | downcase | replace: ', ', ',' | replace: ' ,', ',' | split: ',' -%}
          {% render 'mega-menu', section: section, custom_class: '!tw-w-fit' %}
        {%- endif -%}
      {%- endif -%}
    {%- endif -%}

    {%- if section.settings.enable_search -%}
    {%- if settings.enable_predictive_search -%}
    <link rel="stylesheet" href="{{ 'predictive-search.css' | asset_url }}"{% unless show_search_types %} media="print" onload="this.media='all'"{% endunless %}>
      <script src="{{ 'predictive-search.js' | asset_url }}" defer="defer"></script>
      <script src="{{ 'tabs.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}
    {%- if section.settings.main_menu_alignment == 'left-center' -%}
      <div class="section-cs-search">
        <div class="search-contain">
          <div class="search-contain-inner">
            {%- endif -%}
            {% render 'search-menu', show_search_types: show_search_types, search_is_minimised: search_is_minimised %}
            {%- if section.settings.main_menu_alignment == 'left-center' -%}
            <div class="trigger-show-search !tw-cursor-pointer" onclick="triggerBtn(this, 'a.js-show-search')">
              <span>{{ 'general.icon_labels.cancel_search' | t }}</span>
              <span class="visually-hidden">{{ 'general.icon_labels.search' | t }}</span>
            </div>
          </div>
        </div>
        <div class="overlay-cs" onclick="triggerBtn(this, 'a.js-show-search')"></div>
      </div>
    {%- endif -%}

    {%- elsif section.settings.logo_center or section.settings.main_menu_alignment == 'left-center' -%}
      <div class="header__search"></div>
    {%- endif -%}

    <div class="header__icons flex justify-end mis-auto js-closes-menu {% if section.settings.main_menu_alignment == 'left-center' %}!tw-m-0 {% endif %}
    ">
      {%- if section.settings.enable_search and search_is_minimised -%}
        {%- unless section.settings.logo_center and section.settings.minimise_search_mobile == false and section.settings.minimise_search_desktop -%}
          {%- liquid
            if section.settings.logo_center and section.settings.minimise_search_mobile
              assign search_button_class = "md:hidden"
            elsif section.settings.minimise_search_mobile and section.settings.minimise_search_desktop == false
              assign search_button_class = "md:hidden"
            elsif section.settings.minimise_search_mobile == false and section.settings.minimise_search_desktop
              assign search_button_class = "hidden md:block"
            endif
          -%}
          <a style="color: {{ section.settings.text_color }}" class="header__icon {{ search_button_class }} js-show-search" href="{{ routes.search_url }}" aria-controls="search-bar">
            {% render 'icon-search' %}
            <span class="visually-hidden">{{ 'general.icon_labels.search' | t }}</span>
          </a>
        {%- endunless -%}
      {%- endif -%}
      {%- if shop.customer_accounts_enabled -%}
        {%- if customer -%}
          <a class="header__icon text-current" href="{{ routes.account_url }}">
            {% render 'icon-customer' %}
            <span class="visually-hidden">{{ 'sections.header.account' | t }}</span>
          </a>
        {%- else -%}
          <a class="header__icon text-current" href="{{ routes.account_login_url }}">
            {% render 'icon-customer' %}
            <span class="visually-hidden">{{ 'sections.header.log_in' | t }}</span>
          </a>
        {%- endif -%}
      {%- endif -%}

      <a class="header__icon{% if settings.cart_shaking and cart != empty %} header__icon--cart-shake has-motion{% endif %} relative text-current" id="cart-icon" href="{{ routes.cart_url }}"{% if settings.cart_shaking and cart != empty %} data-shake-frequency="{{ settings.cart_shaking_frequency }}"{% endif %}{% if settings.preload_links %} data-no-instant{% endif %}>
        {%- if settings.cart_icon == 'bag' -%}
          {%- render 'icon-bag' -%}
          <span class="visually-hidden">{{ 'sections.header.bag' | t }}</span>
        {%- elsif settings.cart_icon == 'basket' -%}
          {%- render 'icon-basket' -%}
          <span class="visually-hidden">{{ 'sections.header.basket' | t }}</span>
        {%- else -%}
          {%- render 'icon-cart' -%}
          <span class="visually-hidden">{{ 'sections.header.cart' | t }}</span>
        {%- endif -%}

        <div id="cart-icon-bubble">
          {%- if cart != empty -%}
            <div class="header__cart-count absolute bg-theme-text text-theme-bg text-center">
              {%- if cart.item_count < 10000 -%}
                <span aria-hidden="true" class="header__cart-count-number">{{ cart.item_count }}</span>
              {%- endif -%}
              <span class="visually-hidden">{{ 'sections.header.cart_count' | t: count: cart.item_count }}</span>
            </div>
          {%- endif -%}
        </div>
      </a>
    </div>
    {%- if section.settings.main_menu_alignment == 'left-center' -%}
      {%- if section.settings.mobile_menu_position == 'burger-left' -%}
        <style>
          .overflow-hidden{
            padding-right: 0 !important;
          }
        </style>
        {%- if section.settings.menu != blank -%}
          {%- assign total_promo_count = 0 -%}
          {%- assign featured_links = section.settings.menu_featured_link | downcase | replace: ', ', ',' | replace: ' ,', ',' | split: ',' -%}
          {% render 'mega-menu', section: section, custom_class: '!tw-w-fit', disable_search_trigger: true %}
        {%- endif -%}
      {%- endif -%}
    {%- endif -%}

    {%- if section.settings.main_menu_alignment == 'center-split' -%}
      {%- if section.settings.menu != blank -%}
        {%- assign total_promo_count = 0 -%}
        {%- assign featured_links = section.settings.menu_featured_link | downcase | replace: ', ', ',' | replace: ' ,', ',' | split: ',' -%}
        {% render 'mega-menu', section: section %}
      {%- endif -%}
    {%- endif -%}
  </header>
</store-header>

{% if template.name == "index" %}
  <script>
    const storeHeader = document.querySelector("#storeHeader")
    const storeHeaderGroup = storeHeader.closest('.shopify-section-group-header-group')
    const storeHeaderLinks = document.querySelectorAll(".storeHeaderLink")
    const storeHeaderLinkActive = document.querySelector("#storeHeaderLinkActive")
    const storeHeaderIcons = document.querySelectorAll(".header__icon")
    const storeHeaderLogo = document.querySelector("#storeHeaderLogo")
    const storeHeaderHamburguer = document.querySelector(".storeHeaderHamburguer")
    {% if section.settings.enable_tranparent_header == true %}
      document.addEventListener("DOMContentLoaded", () => {
        storeHeader.style.backgroundColor = "rgb(255, 255, 255, 0)";
        for (let i = 0; i < storeHeaderLinks.length; i++) {
          storeHeaderLinks[i].style.color = "{{ section.settings.text_color_home }}"
          storeHeaderLinkActive.style.color = "{{ section.settings.text_color_home }}"
        }
        for (let i = 0; i < storeHeaderIcons.length; i++) {
          storeHeaderIcons[i].style.color = "{{ section.settings.text_color_home }}"
        }
        storeHeaderHamburguer.style.color = "{{ section.settings.text_color_home }}"
        storeHeaderLogo.style.filter = "invert(100%)";
      })
    {% endif %}
    window.addEventListener('scroll', function() {
      if (window.scrollY > 0) {
        storeHeader.style.backgroundColor = "{{ section.settings.bg_color }}";
        storeHeader.classList.add("scrolled");
        storeHeaderGroup.classList.add("is-scrolled");
        for (let i = 0; i < storeHeaderLinks.length; i++) {
          storeHeaderLinks[i].style.color = "{{ section.settings.text_color_scroll }}"
        }
        storeHeaderLinkActive.style.color = "{{ section.settings.text_color_scroll }}"
        storeHeaderHamburguer.style.color = "{{ section.settings.text_color_scroll }}"
        for (let i = 0; i < storeHeaderIcons.length; i++) {
          storeHeaderIcons[i].style.color = "{{ section.settings.text_color_scroll }}"
        }
        storeHeaderLogo.style.filter = "invert(0%)";
      } else {
          storeHeader.classList.remove("scrolled");
          storeHeaderGroup.classList.remove("is-scrolled");
        {% if section.settings.enable_tranparent_header == true %}
          storeHeader.style.backgroundColor = "rgb(255, 255, 255, 0)";
          for (let i = 0; i < storeHeaderLinks.length; i++) {
            storeHeaderLinks[i].style.color = "{{ section.settings.text_color_home }}"
          }
          storeHeaderLinkActive.style.color = "{{ section.settings.text_color_home }}"
          storeHeaderHamburguer.style.color = "{{ section.settings.text_color_home }}"
          for (let i = 0; i < storeHeaderIcons.length; i++) {
            storeHeaderIcons[i].style.color = "{{ section.settings.text_color_home }}"
          }
          storeHeaderLogo.style.filter = "invert(100%)";
        {% endif %}
      }
    });
    storeHeader.addEventListener("mouseenter", () => {
      storeHeader.style.backgroundColor = "{{ section.settings.bg_color }}"
      for (let i = 0; i < storeHeaderLinks.length; i++) {
        storeHeaderLinks[i].style.color = "{{ section.settings.text_color_scroll }}"
      }
      storeHeaderLinkActive.style.color = "{{ section.settings.text_color_scroll }}"
      storeHeaderHamburguer.style.color = "{{ section.settings.text_color_scroll }}"
      for (let i = 0; i < storeHeaderIcons.length; i++) {
        storeHeaderIcons[i].style.color = "{{ section.settings.text_color_scroll }}"
      }
      storeHeaderLogo.style.filter = "invert(0%)";
    })
    storeHeader.addEventListener("mouseleave", () => {
      if (window.scrollY > 0) {
        storeHeader.style.backgroundColor = "{{ section.settings.bg_color }}"
        for (let i = 0; i < storeHeaderLinks.length; i++) {
          storeHeaderLinks[i].style.color = "{{ section.settings.text_color_scroll }}"
        }
        storeHeaderLinkActive.style.color = "{{ section.settings.text_color_scroll }}"
        storeHeaderHamburguer.style.color = "{{ section.settings.text_color_scroll }}"
        for (let i = 0; i < storeHeaderIcons.length; i++) {
          storeHeaderIcons[i].style.color = "{{ section.settings.text_color_scroll }}"
        }
        storeHeaderLogo.style.filter = "invert(0%)";
      } else {
        {% if section.settings.enable_tranparent_header == true %}
          storeHeader.style.backgroundColor = "rgb(255, 255, 255, 0)";
          for (let i = 0; i < storeHeaderLinks.length; i++) {
            storeHeaderLinks[i].style.color = "{{ section.settings.text_color_home }}"
          }
          storeHeaderLinkActive.style.color = "{{ section.settings.text_color_home }}"
          storeHeaderHamburguer.style.color = "{{ section.settings.text_color_home }}"
          for (let i = 0; i < storeHeaderIcons.length; i++) {
            storeHeaderIcons[i].style.color = "{{ section.settings.text_color_home }}"
          }
          storeHeaderLogo.style.filter = "invert(100%)";
        {% endif %}
      }
    })
  </script>
{% else %}
  <script>
    const storeHeader = document.querySelector("#storeHeader")
    const storeHeaderGroup = storeHeader.closest('.shopify-section-group-header-group')
    window.addEventListener("scroll", () => {
      if (window.scrollY > 0) {
        storeHeader.classList.add("scrolled");
        storeHeaderGroup.classList.add("is-scrolled");
      } else {
        storeHeader.classList.remove("scrolled");
        storeHeaderGroup.classList.remove("is-scrolled");
      }
    })
  </script>
{% endif %}

{%- if has_mega_nav_pills -%}
  <link rel="stylesheet" href="{{ 'navigation-mega-pills.css' | asset_url }}" media="print" onload="this.media='all'">
{%- endif -%}

{%- if has_mega_nav_columns -%}
  <link rel="stylesheet" href="{{ 'navigation-mega-columns.css' | asset_url }}" media="print" onload="this.media='all'">
{%- endif -%}

{%- if has_mega_nav_sidebar -%}
  <link rel="stylesheet" href="{{ 'navigation-mega-sidebar.css' | asset_url }}" media="print" onload="this.media='all'">
{%- endif -%}

{%- if total_promo_count > 0 -%}
  <link rel="stylesheet" href="{{ 'promos.css' | asset_url }}" media="print" onload="this.media='all'">
  <link rel="stylesheet" href="{{ 'navigation-promos.css' | asset_url }}" media="print" onload="this.media='all'">
{%- endif -%}

{% javascript %}
customElements.whenDefined('details-disclosure').then(() => {
class NavMenu extends DetailsDisclosure {
constructor() {
super();

if(this.matches('.js-mega-nav')) {
this.reset(false);
window.addEventListener('on:breakpoint-change', this.reset.bind(this));
}
}

reset(resize = true) {
const isLargeScreen = window.matchMedia(theme.mediaQueries.md).matches;
this.disclosure.open = isLargeScreen;
if (isLargeScreen) {
this.toggle.setAttribute('tabindex', '-1');
} else {
this.toggle.removeAttribute('tabindex');
}
}

handleToggle(e) {
if (!(this.matches('.js-mega-nav') && window.matchMedia(theme.mediaQueries.md).matches)) {
super.handleToggle(e);
}
}
}

customElements.define('nav-menu', NavMenu);
});
{% endjavascript %}

