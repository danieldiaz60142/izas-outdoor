  <script>
    // prettier-ignore
    "use strict";
    (() => {
      window.Instant = window.Instant || { initialized: !1, version: '1.0.0' };
      if (!window.Instant.initialized) {
        window.Instant.initialized = !0;
        let d = (r) => {
            let n = document.getElementById(`i${r}`);
            n && n.scrollIntoView({ behavior: 'smooth', block: 'start' });
          },
          s = (r, n) => {
            let c = r.split('/').pop();
            n.setAttribute('name', `__tt_embed__${c}`),
              window.addEventListener('message', (e) => {
                try {
                  let t = JSON.parse(e.data);
                  t.height &&
                    document
                      .querySelector(`[name="${t.signalSource}"]`)
                      ?.closest('.video__container')
                      ?.setAttribute(
                        'style',
                        `height:${t.height}px;width:325px;border-radius:8px 8px 8px 8px;overflow:hidden`
                      );
                } catch {}
              });
          },
          l = () => {
            let r = document.querySelectorAll('a[data-instant-action-type]');
            Array.from(r).forEach((e) => {
              e.addEventListener('click', (t) => {
                t.preventDefault(), t.stopPropagation();
                let i = t.currentTarget;
                if (!i || !(i instanceof HTMLElement)) return null;
                let o = i.getAttribute('data-instant-action-type'),
                  a = i.getAttribute('data-instant-action-id');
                switch (o) {
                  case 'add-to-cart':
                    fetch(`${window.Shopify?.routes?.root}cart/add.js`, {
                      method: 'POST',
                      headers: { 'Content-Type': 'application/json' },
                      body: JSON.stringify({ items: [{ id: a, quantity: 1 }] }),
                    }).catch(() => {
                      'href' in i && typeof i.href == 'string' && i.href && (window.location.href = i.href);
                    });
                    break;
                  case 'scroll-to-section':
                    a && d(a);
                    break;
                  default:
                    console.warn(`Unknown instant action type: ${o}`);
                }
                return !1;
              });
            });
            let n = document.querySelectorAll('article.video__wrapper');
            Array.from(n).forEach((e) => {
              e.addEventListener('click', () => {
                if (!e.querySelector('iframe')) {
                  let t = e.getAttribute('data-src') ?? '',
                    i = e.getAttribute('data-title') ?? '',
                    o = document.createElement('iframe');
                  (o.className = 'video__iframe'),
                    o.setAttribute('src', t),
                    o.setAttribute('title', i),
                    o.setAttribute('frameborder', '0'),
                    o.setAttribute('allow', 'accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture'),
                    o.setAttribute('allowfullscreen', 'allowfullscreen'),
                    t?.includes('tiktok') && s(t, o),
                    e.appendChild(o);
                }
              });
            });
            let c = document.querySelectorAll('iframe.tiktok__autoplay');
            Array.from(c).forEach((e) => {
              let t = e.getAttribute('src') ?? '';
              s(t, e);
            });
          };
        document.addEventListener('DOMContentLoaded', () => {
          document.readyState !== 'loading' && l();
        });
      }
    })();
  </script>
